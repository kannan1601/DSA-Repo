#include<bits/stdc++.h>
using namespace std;
int maxsubarray(vector<int> &a, int &start, int &end)
{
    int maxTill = 0, maxsofar = a[0];
    int n = a.size();
    for(int i = 0; i < n; i++)
    {
        maxTill+=a[i];
        if(maxTill > maxsofar)
        {
            maxsofar = maxTill;
            end = i;
        }
        if(maxTill < 0)
        {
            maxTill = 0;
            start = i + 1;
        }
    }
    return maxsofar;
}
int main()
{
    
    vector<int> a = {-2,1,-3,4,-1,2,1,-5,4};
    int start = 0, end = 0;
    cout << maxsubarray(a, start, end) << "\n";
    
    for(int i = start; i <= end; i++)
    {
        cout << a[i] <<" ";
    }
        
}
